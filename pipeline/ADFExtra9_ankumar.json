{
	"name": "ADFExtra9_ankumar",
	"properties": {
		"description": "Load Employee.csv file into the NewEmployee table in SQL Db in such a way that automatically it will add one extra column Loadate with value equal to current timestamp of data load.",
		"activities": [
			{
				"name": "EmployeeToNewEmployeeTable",
				"description": "Load Employee.csv file into the NewEmployee table in SQL Db in such a way that automatically it will add one extra column Loadate with value equal to current timestamp of data load.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"additionalColumns": [
							{
								"name": "Loadate",
								"value": {
									"value": "@utcNow()",
									"type": "Expression"
								}
							}
						],
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "EmployeeCSVDS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "GenericSQLDS_ankumar",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-07-18T08:31:49Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}