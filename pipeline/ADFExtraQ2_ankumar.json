{
	"name": "ADFExtraQ2_ankumar",
	"properties": {
		"description": "There is one Employee csv file is attached. Load this csv file in ADLS. Then move the data from CSV to SQL db. Ensure the table is automatically created by the ADF itself.\n",
		"activities": [
			{
				"name": "EmployeeDataToSQL",
				"description": "Move the data from CSV to SQL db. Ensure the table is automatically created by the ADF itself.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "delete from empschema.emptable",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "EmployeeCSVDS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "EmployeeSQLDS_ankumar",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-07-16T14:30:35Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}