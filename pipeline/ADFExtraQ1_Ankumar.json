{
	"name": "ADFExtraQ1_Ankumar",
	"properties": {
		"description": "Fetch all the records from the SQL Db SalesLT.SalesOrderDetail table to ADLS where Unit price is less than the highest unit price.",
		"activities": [
			{
				"name": "Records from Order Detail",
				"description": "Fetch all the records from the SQL Db SalesLT.SalesOrderDetail table to ADLS where Unit price is less than the highest unit price.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "select * from [SalesLT].[SalesOrderDetail] Where UnitPrice < (select MAX(UnitPrice) from [SalesLT].[SalesOrderDetail])",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SQLDatasetankumar",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CSVDSGeneric_ankumar",
						"type": "DatasetReference",
						"parameters": {
							"folderPath": "OrderDetail"
						}
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-07-15T15:29:50Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}