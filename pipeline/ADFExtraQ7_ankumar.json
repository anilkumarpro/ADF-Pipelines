{
	"name": "ADFExtraQ7_ankumar",
	"properties": {
		"description": "There is one Employee csv file has attached.\nLoad this csv file in ADLS. Then move the data from CSV to SQL db. Ensure table is automatically create by the ADF itself. Once data get copied, delete the file using the ADF delete activity.\n",
		"activities": [
			{
				"name": "EmployeeDataToSQL",
				"description": "Load this csv file in ADLS. Then move the data from CSV to SQL db. Ensure table is automatically create by the ADF itself. Once data get copied, delete the file using the ADF delete activity.\n",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "EmployeeCSVDS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "EmployeeSQLDS_ankumar",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Deleting the file using the ADF Delete",
				"description": "delete the file using the ADF delete activity.",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "EmployeeDataToSQL",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "EmployeeCSVDS",
						"type": "DatasetReference"
					},
					"logStorageSettings": {
						"linkedServiceName": {
							"referenceName": "CSVLSankumar",
							"type": "LinkedServiceReference"
						},
						"path": "landing/Employee"
					},
					"enableLogging": true,
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					}
				}
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-07-17T04:02:15Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}